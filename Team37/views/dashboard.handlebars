<div class="dashboard">
    <div class="sidebar">
        <p>This is in the sidebar</p>
        <p>This is also in the sidebar</p>
    </div>

    <div class="dashboard-header">
        this is the dashboard header
    </div>

    <div class="clients">
        <label for="client-search" class="search-label">Search for a client</label>
        <form action="" class="search-form">
            <input type="search" name="client-search" id="client-search" class="item-search-box"
                   placeholder="Search For Something">
            <input type="submit" value="Search" class="item-search-button">
        </form>

        <div class="item-area">
            {{#each clients}}
                <a href="" class="client">{{this}}</a>
            {{/each}}
        </div>
        <div class="item-toolbar">
            <button type="button">Add</button>
            <button type="button">Remove</button>
            <button type="button">Update</button>
        </div>
    </div>

    <div class="props">
        <div class="prop-search">
            <label for="prop-search" class="search-label">Search for an item</label>
            <form action="" class="search-form">
                <input type="search" name="prop-search" id="prop-search" class="item-search-box"
                       placeholder="Search For Something">
                <input type="submit" value="Search" class="item-search-button">
            </form>
        </div>
        <div class="item-area">
            {{#each props}}
                <a href="" class="prop">{{this}}</a>
            {{/each}}
        </div>
        <div class="item-toolbar">
            <a href="/admin/add-prop">Add</a>
            <button type="button">Remove</button>
            <button type="button">Update</button>
        </div>
    </div>
</div>